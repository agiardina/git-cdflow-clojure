<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.web.*?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TreeItem?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.web.WebView?>

<BorderPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cdflow.controller">
   <top>
        <MenuBar id="menuBar" focusTraversable="true" BorderPane.alignment="CENTER">
            <menus>
                <Menu text="File">
                    <items>
                        <MenuItem onAction="#onOpen" text="Open" />
                        <SeparatorMenuItem />
                        <MenuItem text="Exit" />
                    </items>
                </Menu>
                <Menu text="JavaFX 2.0 Examples">
                    <items>
                        <MenuItem text="Text Example" />
                        <MenuItem text="Objects Example" />
                        <MenuItem text="Animation Example" />
                    </items>
                </Menu>
                <Menu text="Help">
                    <items>
                        <MenuItem disable="true" text="Search" />
                        <MenuItem text="Online Manual" visible="false" />
                        <SeparatorMenuItem />
                        <MenuItem text="About" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
   </top>
   <left>
      <SplitPane prefHeight="800.0" prefWidth="300.0" BorderPane.alignment="CENTER">
         <items>
            <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                <Tab text="Local">
                  <content>
                      <TreeView fx:id="branches" editable="true" prefHeight="798.0" prefWidth="0.0" showRoot="false">
                          <root>
                              <TreeItem fx:id="branchesroot" value="Root">
                                  <children>
                                      <TreeItem value="Release" />
                                      <TreeItem value="Prova" />
                                  </children>
                              </TreeItem>
                          </root>
                      </TreeView>
                  </content>
                </Tab>
                <Tab text="Origin">
                  <content>
                      <TreeView fx:id="branchesorigin" editable="true" prefHeight="798.0" prefWidth="0.0" showRoot="false">
                        <root>
                            <TreeItem fx:id="branchesrootorigin" value="Root">
                                <children>
                                    <TreeItem value="Release" />
                                    <TreeItem value="Prova" />
                                </children>
                            </TreeItem>
                        </root>
                      </TreeView>
                  </content>
                </Tab>
              </tabs>
            </TabPane>
         </items>
      </SplitPane>
   </left>
   <center>
      <SplitPane dividerPositions="0.5" orientation="VERTICAL">
         <items>
            <TableView fx:id="commitstable" prefHeight="200.0" prefWidth="200.0">
              <columns>
                <!--<TableColumn fx:id="commitstableDescription" prefWidth="75.0" text="Description" />-->
                <!--<TableColumn fx:id="commitstableCommit" prefWidth="75.0" text="Commit" />-->
                <!--<TableColumn fx:id="commitstableAuthor" prefWidth="75.0" text="Author" />-->
                <!--<TableColumn fx:id="commitstableDate" prefWidth="75.0" text="Date" />-->
              </columns>
            </TableView>
                <WebView fx:id="webview" prefHeight="900.0" prefWidth="0.0" BorderPane.alignment="CENTER" />
         </items>
      </SplitPane>
   </center>
   <!--<right>-->
          <!--<Button fx:id="save" minWidth="-Infinity" mnemonicParsing="false" onAction="#onLoad" text="Load" BorderPane.alignment="CENTER" />-->
   <!--</right>-->
</BorderPane>
